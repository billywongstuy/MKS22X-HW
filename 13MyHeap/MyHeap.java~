import java.util.*;

public class MyHeap<T extends Comparable<T>> {

    private T [] ary;
    private boolean isMax;
    private int size = 0;
    
    public MyHeap() {
	 ary = new T[2];
	 isMax = true;
    }


    public MyHeap(boolean m) {
	ary = new T[2];
	isMax = m;
    }


    public void add(T value) {

    }


    public T remove(T value) {

    }


    public void pushup(T value) {

    }


    public void pushdown(T value) {

    }

    public T peek() {
	return ary[1];
    }
    
    public int size() {
	return size;
    }


    public String toString() {
        String s = "[";
	for (int i = 1; i <= size; i++) {
	    if (i == size) {
		s += ary[i];
	    }
	    else {
		s += (ary[i] + ", ");
	    }
	}
	s += "]";
	return s;
    }
    
}
